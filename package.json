{
  "name": "electron-prototype",
  "version": "0.2.0",
  "description": "üéØ Âü∫‰∫éElectronÊê≠Âª∫ÁöÑÂéüÂûãÈ°πÁõÆ",
  "main": "main.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "electron .",
    "debug": "echo 'chrome://inspect/#devices' && electron --inspect-brk .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "AC-angle",
    "appId": "ac-angle.electron-ptototype",
    "directories": {
      "output": "runner"
    },
    "files": [
      "**/*",
      "!**/node_modules/**/*",
      "!**/runner/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AC-angle/Electron-prototype.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "yelloxing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AC-angle/Electron-prototype/issues"
  },
  "homepage": "https://github.com/AC-angle/Electron-prototype#readme",
  "devDependencies": {
    "@yelloxing/normalize.css": "^0.2.2",
    "electron": "^8.2.4",
    "electron-builder": "^22.4.1",
    "image2d": "^1.6.6"
  }
}